# export PATH="$HOME/.rbenv/bin:$PATH"
# eval "$(rbenv init -)"

alias nemo='nemo ./ && exit'

function set_title() {
  if [[ -z "$ORIG" ]]; then
    ORIG=$PS1
  fi
  TITLE="\[\e]2;$*\a\]"
  PS1=${ORIG}${TITLE}
}

EDITOR_PATH=`which subl`
export EDITOR="$EDITOR_PATH -w"

export PS1="${debian_chroot:+($debian_chroot)}\[\033[01;32m\]\u@\h \[\033[01;33m\]$STY \[\033[01;34m\]\w \$\[\033[00m\] "
